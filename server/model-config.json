{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "elastic": {
    "dataSource": false,
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "ping": true,
          "match": true,
          "matchPhrase": true,
          "multiMatchAndFilter": true
        }
      }
    }
  }
}
